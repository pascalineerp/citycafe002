odoo.define('web_studio.AbstractReportComponent',function(require){"use strict";var Widget=require('web.Widget');var AbstractReportComponent=Widget.extend({init:function(parent,params){this.models=params.models;this.node={context:{},contextOrder:[],};this._super.apply(this,arguments);},_getContextKeys:function(node){var self=this;var contextOrder=node.contextOrder||[];var keys=_.compact(_.map(node.context,function(relation,key){if(!self.models[relation]){return{name:key,string:key+' ('+relation+')',type:relation,store:true,related:true,searchable:true,order:-contextOrder.indexOf(key),};}
