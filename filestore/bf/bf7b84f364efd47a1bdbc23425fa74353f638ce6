odoo.define('web_studio.report_fields',function(require){"use strict";var fieldRegistry=require('web.field_registry');var relationalFields=require('web.relational_fields');var FieldMany2ManySelection=relationalFields.FieldMany2ManyTags.extend({init:function(parent,name,record,options){this._super.apply(this,arguments);options.quick_create=false;options.can_create=false;this.selection=_.map(options.attrs.selection,function(s){return{id:s.field_name,res_id:s.field_name,data:{id:s.field_name,display_name:s.label}};});},_isSameValue:function(value){if(value===false){return this.value===false;}
