odoo.define('@web_studio/client_action/app_creator/app_creator_shortcut', async function (require) {
'use strict';
let __exports = {};
/** @odoo-module **/

const { registry } = require("@web/core/registry");

const actionRegistry = registry.category("actions");

actionRegistry.add("action_web_studio_app_creator",
    (env) => env.services.studio.open(env.services.studio.MODES.APP_CREATOR)
);

return __exports;
});

